{
  "name": "ts-express-prisma-boilerplate",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "dependencies": {
    "@prisma/client": "4.4.0",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "husky": "^8.0.1",
    "prisma": "^4.4.0",
    "ts-node": "^10.9.1"
  },
  "scripts": {
